{
  "name": "ad-hoc-doc",
  "displayName": "ad-hoc-doc",
  "description": "An extension that adds ad hoc documentation to your code",
  "version": "0.0.1",
  "publisher": "Morkalork",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ad-hoc-doc.addDocumentation",
        "title": "Add Documentation"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceLangId == typescript || resourceLangId == javascript",
          "command": "ad-hoc-doc.addDocumentation",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "when": "resourceLangId == typescript || resourceLangId == javascript",
          "command": "ad-hoc-doc.addDocumentation",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "release": "vsce publish patch",
    "release-minor": "vsce package && vsce publish minor",
    "release-major": "vsce package && vsce publish major",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@babel/types": "^7.24.6",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "@vscode/vsce": "^2.26.1",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@babel/parser": "^7.24.6",
    "recast": "^0.23.9"
  }
}
